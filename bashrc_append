alias tmux="tmux -2"

function git_branch {
	ref=$(git symbolic-ref HEAD 2> /dev/null) || return;
	echo "("${ref#refs/heads/}") ";
}

happen-grep() { grep -cR "$1" * | grep -v ':0' ;}

