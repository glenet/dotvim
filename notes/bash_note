history					// List command history
history | cut -c 8		// History without line number

grep -nR 'term' ./*		// Search 'term' word in all files under current dir
grep -w 'term'			// Searches for the expression as a word as if surrounded by \< and \>
grep -nR --exclude=*.o 'term' *

// 找出所有包含有 "keyword" 的 java檔
~# : find ./ -name \*.java -exec grep -wl "keyword" {} \; 

// Dump htc ssd test tool system logger's log
adb pull /data/htclog htc_log
cd htc_log/

// Minicom 使用方式
minicom -s	// 更改 serial port
Ctrl+A + L	// 輸出 log 成檔案
Ctrl+A + Q	// 退出
